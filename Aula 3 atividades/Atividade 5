#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
#include <string.h>
int main()
{
    setlocale(LC_ALL,"Portuguese");

    /*5) Escrever um programa em C para ler dois valores e mostrar uma
mensagem se os valores são múltiplos ou não.*/

    int numero1,numero2,resto;

     printf("insira o primeiro numero:");
      scanf("%d",&numero1);

        printf("insira o segundo numero:");
         scanf("%d",&numero2);

    resto=numero1%numero2;

    if (resto==0)
        printf("\n Os numeros são multiplos");

        else
            printf("\n Não são multiplos");


    return 0;
}
