#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
#include <string.h>
int main()
{
    setlocale(LC_ALL,"Portuguese");

    int numero,numero2;

        printf("insira numero 1:");
        scanf("%d",&numero);

        printf("insiran numero 2:");
         scanf("%d", &numero2);

         if(numero>numero2)
            printf("numero 1 maior que numero 2");

         else
                    printf("\nnumero 2 maior que numero 1");




    return 0;
}
