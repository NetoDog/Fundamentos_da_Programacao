#include <stdio.h>
#include <stdlib.h>
#include <locale.h>
#include <math.h>
#include <string.h>
int main()
{
    setlocale(LC_ALL,"Portuguese");

    //Atividade 1 Escreva um vetor que receba 5 numeros, e mostre-os

    int R[5],i;

    for (i=1;i<=5;i++){
        printf("Informe o Numero de R:");
        scanf ("%d",&R[i]);
    }
    for (i=1;i<=5;i++)
    {
        printf("\n Numeros na posição %d : \n %d",i,R[i]);
    }

    return 0;
}
